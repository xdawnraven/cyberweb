{
    "version": 2,
    "builds": [
        {
            "src": "./index.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "/"
        }
    ]
    "rewrites": [
        {
            "source": "/Login_Success", "destination": "/views/dashboard.ejs"
        }
    ]
}
